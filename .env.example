# Discord Bot Token (from Discord Developer Portal)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# LUNA API Endpoint 
# Change this to your production API URL when deploying
LUNA_API_URL=http://localhost:5000/api

# Workflow ID to use in LUNA
# This should match your existing Discord operator workflow in your Neo4j database
LUNA_WORKFLOW_ID=discord_operator
